{
  "editor.formatOnType": true,
  "editor.bracketPairColorization.enabled": true,
  "workbench.colorTheme": "FlatUI Dark",
  "workbench.iconTheme": "material-icon-theme",
  "tabnine.experimentalAutoImports": true,
  "GitLive.Issue tracker integration": "Disabled",
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "[yaml,yml]": {
    "editor.formatOnSave": false
  },
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "files.autoGuessEncoding": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "files.useExperimentalFileWatcher": true,
  "files.exclude": {
    "/.git": true,
    "/.DS_Store": true,
    "/node_modules": true,
    "/node_modules/": true
  },
  "search.exclude": {
    "/node_modules": true
  },
  "files.watcherExclude": {
    "/node_modules/": true
  },
  "vs-kubernetes": {
    "vscode-kubernetes.helm-path.mac": "/Users/ishikawanaohiro/.vs-kubernetes/tools/helm/darwin-arm64/helm",
    "vscode-kubernetes.minikube-path.mac": "/Users/ishikawanaohiro/.vs-kubernetes/tools/minikube/darwin-arm64/minikube",
    "vscode-kubernetes.kubectl-path.mac": "/Users/ishikawanaohiro/.vs-kubernetes/tools/kubectl/kubectl"
  },
  "redhat.telemetry.enabled": true
}
